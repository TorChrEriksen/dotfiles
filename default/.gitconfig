[user]
  name =
  email =
[color "diff"]
  whitespace = red reverse
[color]
  diff = auto
  ui = auto
[push]
  default = simple
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  cp = cherry-pick
  sh = show
  l = log
  rso = remote show origin
