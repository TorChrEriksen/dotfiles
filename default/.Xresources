! #include ".Xresources.d/rxvt-unicode_solarized-light"
! #include ".Xresources.d/rxvt-unicode_solarized-dark"

!===============================================================================
! Solarized color scheme for the X Window System
!
! http://ethanschoonover.com/solarized

!===============================================================================
! Common color stuff
! colors 0-7 are normal colours
! colors 8-15 are bright counterparts


! Common

#define yellow        #b58900
#define orange        #cb4b16
#define red           #dc322f
#define magenta       #d33682
#define violet        #6c71c4
#define blue          #268bd2
#define cyan          #2aa198
#define green         #859900

! xterm
! #define yellow        #af8700
! #define orange        #d75f00
! #define red           #d70000
! #define magenta       #af005f
! #define violet        #5f5faf
! #define blue          #0087ff
! #define cyan          #00afaf
! #define green         #5f8700


! Dark (base1 can be used as a 5th for optional emphasized content)
! base03 background
! base02 background_highlights
! base01 comments/secondary content
! base0  body text/default code/primary content
#define base03        #002b36
#define base02        #073642
#define base01        #586e75
#define base0         #839496

! Light (base01 can be used as a 5th for optional emphasized content)
! base3  background
! base2  background_highlights
! base1  comments/secondary content
! base00 body text/default content/primary content
#define base3         #fdf6e3
#define base2         #eee8d5
#define base1         #93a1a1
#define base00        #657b83


! To only apply colors to your terminal, for example, prefix
! the color assignment statement with its name. Example:
!
! URxvt*background:            S_base03

!dark
*background:              base03
*foreground:              base0
*cursorColor:             base1

!light
! *background:              base3
! *foreground:              base00
! *cursorColor:             base01

! *fading:                  40
*fadeColor:               base03
! *pointerColorBackground:  base01
! *pointerColorForeground:  base1

! color1 = archive
! color2 = executable
! color12 = dir
! color13 = socket

! *color0:                  base02
*color1:                  red
*color2:                  green
! *color3:                  yellow
! *color4:                  blue
! *color5:                  magenta
! *color6:                  cyan
! *color7:                  base2
! *color9:                  orange
! *color8:                  base03
*color10:                 red
*color11:                 yellow
! *color10:                 base01
! *color11:                 base00
*color12:                 blue
*color13:                 violet
! *color14:                 base1
! *color15:                 base3
!===============================================================================

